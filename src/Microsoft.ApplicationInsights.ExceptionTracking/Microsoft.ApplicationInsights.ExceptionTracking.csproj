    <PackageLicenseUrl>https://www.microsoft.com/web/webpi/eula/net_library_eula_enu.htm</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Microsoft/ApplicationInsights-ExceptionTracking</PackageProjectUrl>
    <PackageTags>Microsoft;Diagnostics;ApplicationInsights;Exceptions</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>

</Project>
