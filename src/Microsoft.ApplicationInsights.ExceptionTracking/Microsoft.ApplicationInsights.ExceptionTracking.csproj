    <AssemblyName>Microsoft.ApplicationInsights.ExceptionTracking</AssemblyName>
    <RootNamespace>Microsoft.ApplicationInsights.ExceptionTracking</RootNamespace>
    <AuthenticodeSignTarget>true</AuthenticodeSignTarget>

    <DebugType>pdbonly</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <Authors>Microsoft</Authors>
